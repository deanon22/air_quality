<div class="jumbotron <%= @api_color %>">
  <h1 class="display-4">
    <% if @output.empty? %>
      <%= @myerrormsg %>
    <% else %>
      Air Quality Index: <%= @final_output[0]['AQI'] %> </br>
      Reporting Area: <%= @final_output[0]['ReportingArea'] %></br>
      Category Number: <%= @final_output[0]['Category']['Number']%> </br>
      Date Observed: <%= @final_output[0]['DateObserved'] %></br>
      Current Time: <%= @Now %>
  </h1>
        <p class="lead">This is the current Air Quality Score for <%= @final_output[0]['ReportingArea']%>, <%= @final_output[0]['StateCode']%>: <%= @final_output[0]['AQI'] %></p>
        <hr class="my-4">
    <% end %> 
  </div>  
</br>
  
